{
  "dynamic_templates": [
    {
      "i18n_names": {
          "match_pattern": "regex",
          "path_match": "^name($|s\\.\\w+)",
          "mapping": {
              "type": "text",
              "analyzer": "word",
              "fields": {
                  "prefix": {
                      "type": "text",
                      "analyzer": "prefix",
                      "search_analyzer": "word"
                  }
              }
          }
      }
    },
    {
      "i18n_labels": {
          "match_pattern": "regex",
          "path_match": "^label($|s\\.\\w+)",
          "mapping": {
              "type": "text",
              "analyzer": "word",
              "copy_to": "full_label",
              "norms": false,
              "fields": {
                  "prefix": {
                      "type": "text",
                      "analyzer": "prefix",
                      "search_analyzer": "word",
                      "norms": false
                  },
                  "ngram": {
                      "type": "text",
                      "analyzer": "ngram",
                      "search_analyzer": "ngram",
                      "norms": false
                  }
              }
          }
        }
    },
    {
      "dynamic_objects": {
        "match_mapping_type": "object",
        "mapping": {
          "dynamic": true
        }
      }
    },
    {
      "disable_other_dynamic_fields": {
          "match_mapping_type": "*",
          "mapping": {
            "type": "object",
            "dynamic": false,
            "enabled": false
          }
      }
    }
  ],
  "properties": {
    "id": {
      "type": "keyword",
      "index": false
    },
    "level": {
      "type": "long",
      "index": false
    },
    "zip_codes": {
      "type": "text",
      "index_options": "docs",
      "analyzer": "word",
      "copy_to": "full_label"
    },
    "coord": {
      "type": "geo_point"
    },
    "approx_coord": {
      "type": "geo_shape"
    },
    "full_label": {
      "type": "text",
      "index": false,
      "norms": false,
      "fields": {
        "prefix": {
          "type": "text",
          "analyzer": "prefix_elision",
          "search_analyzer": "word_elision",
          "norms": false
        },
        "ngram": {
          "type": "text",
          "analyzer": "ngram",
          "search_analyzer": "ngram",
          "norms": false
        }
      }
    },
    "weight": {
      "type": "double"
    },
    "zone_type": {
      "type": "keyword"
    },
    "indexed_at": {
      "type": "date"
    },
    "administrative_regions": {
      "enabled": false
    },
    "boundary": {
      "enabled": false
    },
    "country_codes": {
      "type": "keyword"
    },
    "insee": {
      "type": "keyword"
    },
    "parent_id": {
      "type": "keyword"
    },
    "codes": {
      "type": "flattened"
    },
    "type": {
      "type": "constant_keyword"
    }
  }
}
