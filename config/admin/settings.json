{
  "refresh_interval": "60s",
  "analysis": {
    "filter": {
      "prefix_filter": {
        "type": "edge_ngram",
        "min_gram": 1,
        "max_gram": 10
      },
      "elision_filter" : {
        "type" : "elision",
        "articles" : ["l", "d"]
      }
    },
    "analyzer": {
      "word": {
        "tokenizer": "standard",
        "filter": [ "lowercase", "asciifolding" ],
        "char_filter" : [ ]
      },
      "word_elision": {
        "tokenizer": "standard",
        "filter": [ "lowercase", "elision_filter", "asciifolding" ],
        "char_filter" : [ ]
      },
      "prefix": {
        "tokenizer": "standard",
        "filter": [ "lowercase", "asciifolding", "prefix_filter" ],
        "char_filter" : [ ]
      },
      "prefix_elision": {
        "tokenizer": "standard",
        "filter": [ "lowercase", "asciifolding", "elision_filter", "prefix_filter" ],
        "char_filter" : [ ]
      },
      "autocomplete": {
        "tokenizer": "autocomplete",
        "filter": [ "lowercase", "asciifolding", "elision_filter" ]
      },
      "autocomplete_search": {
        "tokenizer": "standard",
        "filter": [ "lowercase", "asciifolding", "elision_filter" ]
      }
    },
    "tokenizer": {
      "autocomplete": {
        "type": "edge_ngram",
        "min_gram": "2",
        "max_gram": "10",
        "token_chars": [ "letter", "digit" ]
      }
    }
  },
  "index.default_pipeline": "indexed_at"
}
